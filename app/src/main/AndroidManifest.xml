<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    package="com.ace.c11flight"
    xsi:schemaLocation="http://schemas.android.com/apk/res/android ">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NOTIFICATION_POLICY" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/ic_splash"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_splash"
        android:supportsRtl="true"
        android:theme="@style/Theme.C11Flight"
        tools:targetApi="31">
        <activity
            android:name=".ui.view.PromoListActivity"
            android:exported="false" />
        <activity
            android:name=".ui.view.TicketListActivity"
            android:exported="false" />
        <activity
            android:name=".ui.view.MyBrodcastActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".ui.view.WishlistActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".ui.view.SelectFlightActivity"
            android:exported="false" />
        <activity
            android:name=".ui.view.PaymentPageActivity"
            android:exported="false" />
        <activity
            android:name=".ui.view.SelectDateActivity"
            android:exported="false" />
        <activity
            android:name=".ui.view.TicketActivity"
            android:value="" />

        <meta-data
            android:name="android.app.lib_name"
            android:value="" />

        <activity
            android:name=".ui.view.OrderDetailActivity"
            android:exported="false"
            tools:ignore="MissingClass">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".ui.view.SplashScreenActivity"
            android:exported="true"
            android:theme="@style/Theme.C11Flight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".ui.view.AirportListActivity"
            android:exported="false"
            tools:ignore="WrongManifestParent">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".ui.view.BookingActivity"
            android:exported="true"
            android:label="@string/title_activity_booking"
            android:theme="@style/Theme.C11Flight.NoActionBar">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".ui.view.EditProfileActivity"
            android:exported="false" />
        <activity
            android:name=".ui.view.ProfileActivity"
            android:exported="false" />
        <activity
            android:name=".ui.view.ResetPassword"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".ui.view.HomeActivity"
            android:exported="true">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".ui.view.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".ui.view.RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />
    </application>

</manifest>